   -
     code : |1-
      Proc√©dure MyWindow()
      
      // Structure to store the data
      STData is Structure
      	dDate			is date
      	nSeries1			is int
      	nSeries2			is int
      end
      
      // Array containing the chart data
      garrData is array of STData
   -
     code : |1+
      InitRandom(11)
      // Defines the first data
      stAGivenData is STData
      stAGivenData:nSeries1		= 50
      stAGivenData:nSeries2		= 300
      stAGivenData:dDate			= FirstDayOfMonth()
      
      // Adds data randomly
      for nSubscript = 1 to 100
      	
      	stAGivenData:dDate..Day++
      	stAGivenData:nSeries1		+= Random(-10,10)
      	stAGivenData:nSeries2		+= Random(-20,20)
      	
      	// Stores this data
      	Add(garrData, stAGivenData)
      	
      end
      
      // No aggregation by default
      grColumnAggregation(CHART_Chart, grhaNone)
      
      // Forces the chart drawing
      grDraw(CHART_Chart)
      
