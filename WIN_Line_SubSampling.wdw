  type_code : 1
  p_codes :
   -
     code : |1-
      Procédure MyWindow()
      
      // Structure to store the data
      STData is Structure
      	dDateTime is datetime
      	nSeries1 is int
      end
      
      // Array containing the chart data
      garrData is array of STData
   -
     code : |1+
      
      // Display mask for X coordinate
      grTimeLevelMask(CHART_Chart, grntMillisecond, "CCC")
      grTimeLevelMask(CHART_Chart, grntSecond, "SS")
      grTimeLevelMask(CHART_Chart, grntMinute, "mm")
      grTimeLevelMask(CHART_Chart, grntHour, "HHh")
      grTimeLevelMask(CHART_Chart, grntDay, "DD")
      
      // The chart displays the hours to the max (the data is spread over a single day)
      grTimeLevelMax(CHART_Chart, 3, grntHour)
      
      // Start and end dates
      dtStart is datetime = Today()+"000000"
      dtEnd is datetime = Today()+"235959"
      
      // Initial value
      nValue1 is int = 50
      
      // Loop to generate random data
      while dtStart <= dtEnd
      	
      	// Defines the data
      	stAData is STData
      	stAData.dDateTime = dtStart
      	stAData.nSeries1 = nValue1
      	// Stores the data
      	Add(garrData, stAData)
      	
      	//  Sets the next value
      	nValue1 += Random(-2,2)
      	dtStart..Second+=5
      	
      end
