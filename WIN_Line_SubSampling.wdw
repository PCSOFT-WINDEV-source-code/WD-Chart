         // If the sampling is enabled
         if grParameter(CHART_Chart, grSamplingEnabled) then
         	
         	// Disables the sampling
         	grParameter(CHART_Chart, grSamplingEnabled, False)
         	
         	// Modifies the caption of the button
         	MySelf.. libellé = "Enable the sampling"
         	
         else // If the sampling is not enabled
         
         	// Enables the sampling
         	grParameter(CHART_Chart, grSamplingEnabled, True)
         	
         	// Modifies the caption of the button
         	MySelf.. libellé = "Disable the sub-sampling"
         
         end
         
         // Refreshes the chart drawing
         grDraw(CHART_Chart)
         
         HourGlass(False)
